extend ./1.base/base.jade

append stylesheet         
    link(href="/src/css/1.base/nav.css", rel="stylesheet")  
    link(href="/src/css/3.prooverview/proprojects.css", rel="stylesheet")
block content
    block nav
        include ./2.includes/nav
    append sections
        div(class="container pro-projects")
            div(class="row")
                div(class="slide-container col-xs-12 col-sm-12 col-md-8 col-lg-8")
                    div(id="carousel-project-items" class="carousel slide" data-ride="carousel" data-interval="false")
                        div(class="carousel-inner" role="listbox")
                            div(class="item active")
                                img(src="/Content/pro/images/bedroom.jpg" alt="")
                            div(class="item")
                                img(src="/Content/pro/images/bedroom.jpg" alt="")
                            div(class="item")
                                img(src="/Content/pro/images/bedroom.jpg" alt="")
                        a(class="left carousel-control" href="#carousel-project-items" role="button" data-slide="prev")
                            span(class="glyphicon glyphicon-chevron-left" aria-hidden="true")
                            span(class="sr-only") Previous
                        a(class="right carousel-control" href="#carousel-project-items" role="button" data-slide="next")
                            span(class="glyphicon glyphicon-chevron-right" aria-hidden="true")
                            span(class="sr-only") Next
                        div(id="list-carousel-project-items" class="row")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="0")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="0")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="1")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                            a(class="indicator-item" data-target="#carousel-project-items" data-slide-to="2")
                                div(class="indicator-item-image" style="background-image: url('/Content/pro/images/bedroom.jpg')")
                div(class="image-items-side col-xs-12 col-sm-12 col-md-4 col-lg-4")
                    div(class="row")
                        div(class="col-xs-12 image-detail")
                            h2(class="img-detail-name") Contemporary Spaces with Doors to Match
                            p(class="img-detail-description") In the master bedroom, the subtle use of color keeps the mood serene. The modern king-sized bed is from B@B Italia. The Willy Dilly Lamp is by Ingo Maurer and the white Oregani linens were purchased at Luminaire.
                        div(class="col-xs-12 image-ask-container")
                            button(class="btn btn-u btn-u-blue btn-u-lg btn-block" data-toggle="modal" data-target="#askModal")
                                i(class="fa fa-comment")
                                |Ask
                        div(class="col-xs-12 image-questions-and-response-container")
                            div(class="image-question-container row")
                                div(class="image-question-details col-xs-12")
                                    div(class="row")
                                        div(class="col-xs-12 question-user")
                                            <img class="rounded-x" src="https://www.casafix.com/cdn/expert/AvatarExpert.jpg" alt="">
                                            span Juan Hermano                                
                                    div(class="row")
                                        div(class="col-xs-12 question-content")
                                            p In the master bedroom, the subtle use of color keeps the mood serene. The modern king-sized bed is from B@B Italia. The Willy Dilly Lamp is by Ingo Maurer and the white Oregani linens were purchased at Luminaire.
                                    div(class="row")
                                        div(class="col-xs-12 question-action")
                                            ul
                                                li
                                                    a(class="link-like" href="")
                                                        i(class="fa fa-thumbs-o-up fa-1x" aria-hidden="true")
                                                        span(class="count-like")  2 
                                                        span like
                                                    span  | 
                                                li
                                                    a(class="link-comment" data-toggle="collapse" href="#collapse_comment_1" aria-expanded="false" aria-controls="collapse_comment_1")
                                                        i(class="fa fa-comment-o fa-1x" aria-hidden="true")
                                                        span  Comment
                                                    span  |
                                                li       
                                                    <i class="fa fa-calendar"></i>                                     
                                                    span Jan 7, 2017
                                        div(class="collapse col-xs-12" id="collapse_comment_1")
                                            form( action="")
                                                div(class="form-group")
                                                    div(class="input-group input-group-ask-form")
                                                        span(class="control-label hidden" ) Resources.lblAskIncorrectMessage
                                                        textarea(rows="2" class="form-control ask-form-text" name="text")
                                                        input(type="hidden" name="questionId" value="1")
                                                    div(class="form-group")
                                                        input(class="btn btn-block submit-comment" type="submit" value="Send")
                                div(class="image-question-comments col-xs-12")
                                    each i in new Array(3)
                                        div(class="image-question-comment row")
                                            div(class="col-xs-12 comment-user")
                                                <img class="rounded-x" src="https://www.casafix.com/cdn/expert/AvatarExpert.jpg" alt="">
                                                span Juan Hermano                                            
                                            div(class="col-xs-12 comment-content")
                                                p In the master bedroom, the subtle use of color keeps the mood serene. The modern king-sized bed is from B@B Italia. The Willy Dilly Lamp is by Ingo Maurer and the white Oregani linens were purchased at Luminaire.
                                            div(class="col-xs-12 comment-actions")
                                                ul
                                                    li
                                                        a(class="link-like" href="")
                                                            i(class="fa fa-thumbs-o-up fa-1x" aria-hidden="true")
                                                            span(class="count-like")  2 
                                                            span like
                                                        span  | 
                                                    li   
                                                        <i class="fa fa-calendar"></i>                                         
                                                        span Jan 7, 2017                            


    append footer
        include ./2.includes/footer
    append scripts
        script(src="/src/js/nav.js")